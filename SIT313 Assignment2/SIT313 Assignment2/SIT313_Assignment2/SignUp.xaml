<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SIT313_Assignment2.SignUp">
  <StackLayout Spacing="10" x:Name="layout">
    <Entry Placeholder="Enter Full Name" />
    <Entry Placeholder="Enter Preferred User Name"/>
    <Entry Placeholder="Enter Address"/>
    <Entry Placeholder="Password" />
    <Entry Placeholder="Enter Credit Card Number"/>
    <Entry Placeholder="Enter Credit Card Expiry Date"/>
    <Button Text="Sign Up" />
    </StackLayout>
</ContentPage>